
public class TestaConversao {

	public static void main(String[] args) {
		
		try {
			double salario = 1270.50;
			
			String valor = String.valueOf(salario);
			
			System.out.println(valor);
			
			int a = 5;
			
			for (int i = 0; i <= a; i++) {
				
				System.out.println("laço " + i);
				
				if(i == 4) {
					break;
				}
				
			}
			
		} catch (Exception e) {
			// TODO: handle exception
		}
		
		

	}

}
